# Doctor Vision API Keys - Template File
# Copy this file to .env and fill in your actual API keys

# Roboflow API Key for medical image analysis
# Get your key from: https://roboflow.com/
ROBOFLOW_API_KEY=your_roboflow_api_key_here

# Groq API Key for AI chatbot (Llama3-8b model)
# Get your key from: https://console.groq.com/
GROQ_API_KEY=your_groq_api_key_here

# TomTom Maps API Key for location services
# Get your key from: https://developer.tomtom.com/
TOMTOM_API_KEY=your_tomtom_api_key_here

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True

# Production Configuration (uncomment for deployment)
# FLASK_ENV=production
# FLASK_DEBUG=False

# Database Configuration (if needed)
# DATABASE_URL=your_database_url_here

# Security Configuration
SECRET_KEY=generate_a_secure_secret_key_for_production

# Server Configuration
HOST=0.0.0.0
PORT=5000
